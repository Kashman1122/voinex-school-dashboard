{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "/_next/image(.*)",
      "dest": "/_next/image$1"
    },
    {
      "src": "/(.*?\\..+?)(?<!/|\\.\\w+$)",
      "dest": "/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "output": "standalone",
  "cleanUrls": true,
  "trailingSlash": false
}